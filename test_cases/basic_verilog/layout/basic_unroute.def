VERSION 5.6 ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;
DESIGN basic ;
UNITS DISTANCE MICRONS 100 ;

DIEAREA ( -480 -400 ) ( 34071 2200 ) ;

TRACKS Y -400 DO 14 STEP 200 LAYER metal1 ;
TRACKS X -480 DO 217 STEP 160 LAYER metal2 ;
TRACKS Y -400 DO 14 STEP 200 LAYER metal3 ;
TRACKS X -480 DO 217 STEP 160 LAYER metal4 ;

VIAS 0 ;
END VIAS

COMPONENTS 2 ;
- _1_ INVX1 + PLACED ( 80 100 ) FS ;
- _2_ BUFX2 + PLACED ( 400 100 ) FS ;
END COMPONENTS

PINS 4 ;
- a + NET a
  + LAYER metal3 ( -30 -30 ) ( 30 30 )
  + PLACED ( -320 600 ) N ;
- gnd + NET gnd
  + LAYER metal3 ( -30 -30 ) ( 30 30 )
  + PLACED ( 1280 1600 ) N ;
- o + NET o
  + LAYER metal3 ( -30 -30 ) ( 30 30 )
  + PLACED ( 1280 1200 ) N ;
- vdd + NET vdd
  + LAYER metal2 ( -30 -30 ) ( 30 30 )
  + PLACED ( 480 -200 ) N ;
END PINS

NETS 5 ;
- a
  ( PIN a ) 
  ( _1_ A ) ;
- _0_
  ( _2_ A ) 
  ( _1_ Y ) ;
- o
  ( PIN o ) 
  ( _2_ Y ) ;
- gnd
  ( PIN gnd ) ;
- vdd
  ( PIN vdd ) ;
END NETS

END DESIGN
